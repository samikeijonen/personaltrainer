{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "custom": {
            "color": {
                "orange": "rgb(224, 105, 46)",
                "orange-75": "rgba(224, 105, 46, 0.75)",
                "orange-50": "rgba(224, 105, 46, 0.5)",
                "orange-20": "rgba(224, 105, 46, 0.20)",
                "blue": "rgb(18, 51, 64)",
                "blue-75": "rgba(18, 51, 64, 0.75)",
                "blue-50": "rgba(18, 51, 64, 0.5)",
                "blue-20": "rgba(18, 51, 64, 0.20)",
                "lime": "rgb(209, 232, 112)",
                "lime-75": "rgba(209, 232, 112, 0.75)",
                "lime-50": "rgba(209, 232, 112, 0.5)",
                "lime-20": "rgba(209, 232, 112, 0.20)",
                "sand": "rgb(237, 209, 184)",
                "sand-75": "rgba(237, 209, 184, 0.75)",
                "sand-50": "rgba(237, 209, 184, 0.5)",
                "sand-20": "rgba(237, 209, 184, 0.20)",
                "green": " rgb(24, 64, 59)",
                "green-75": "rgba(24, 64, 59, 0.75)",
                "green-50": "rgba(24, 64, 59, 0.5)",
                "green-20": "rgba(24, 64, 59, 0.20)",
                "grey": {
                    "100": "hsl(40, 23%, 97%)",
                    "200": "hsl(43, 13%, 90%)",
                    "300": "hsl(40, 15%, 80%)",
                    "400": "hsl(39, 11%, 69%)",
                    "500": "hsl(41, 8%, 61%)",
                    "600": "hsl(41, 8%, 44%)",
                    "700": "hsl(41, 9%, 35%)",
                    "800": "hsl(37, 11%, 28%)",
                    "900": "hsl(40, 13%, 23%)",
                    "1000": "hsl(42, 15%, 13%)"
                },
                "neutral": {
                    "white": "#fff",
                    "black": "#000"
                }
            },
            "text": {
                "step-m1": "clamp(0.89rem, calc(0.86rem + 0.16vw), 1.00rem)",
                "step-0": "clamp(1.00rem, calc(0.93rem + 0.36vw), 1.25rem)",
                "step-1": "clamp(1.13rem, calc(1.00rem + 0.64vw), 1.56rem)",
                "step-2": "clamp(1.27rem, calc(1.07rem + 1.00vw), 1.95rem)",
                "step-3": "clamp(1.42rem, calc(1.13rem + 1.48vw), 2.44rem)",
                "step-4": "clamp(1.60rem, calc(1.18rem + 2.11vw), 3.05rem)",
                "step-5": "clamp(1.80rem, calc(1.22rem + 2.93vw), 3.82rem)",
                "step-6": "clamp(2.03rem, calc(1.23rem + 3.99vw), 4.77rem)"
            },
            "line-height": {
                "text": "1.5",
                "heading": "1.2"
            },
			"font": {
				"sans-serif": "Be Vietnam Pro, Arial, sans-serif;"
			},
            "width": {
                "site-max": "125rem",
                "container": "87.5rem",
                "container-wide": "87.5rem",
                "container-medium": "46rem",
                "container-small": "27.5rem"
            },
            "transition": {
                "3": "0.3s",
                "4": "0.4s"
            }
        },
        "border": {
            "color": false,
            "radius": false,
            "style": false,
            "width": false
        },
        "color": {
            "custom": false,
            "customGradient": false,
            "defaultPalette": false,
            "defaultGradients": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": []
        },
        "layout": {
            "contentSize": "var(--wp--custom--width--container-medium)",
            "wideSize": "var(--wp--custom--width--container-wide)"
        },
        "spacing": {
            "margin": false,
            "padding": false,
            "units": []
        },
        "typography": {
            "customFontSize": false,
            "fontStyle": false,
            "fontWeight": false,
            "lineHeight": false,
            "textDecoration": false,
            "textTransform": false,
            "dropCap": false,
            "fontFamilies": [],
            "fontSizes": []
        },
		"blocks": {
            "meomblocks/group": {
                "color": {
                    "palette": [
                        {
                            "slug": "sand-20",
                            "color": "var(--wp--custom--color--sand-20)",
                            "name": "Puuteri 20%"
                        },
                        {
                            "slug": "sand-50",
                            "color": "var(--wp--custom--color--sand-50)",
                            "name": "Puuteri 50%"
                        },
                        {
                            "slug": "blue-20",
                            "color": "var(--wp--custom--color--blue-20)",
                            "name": "Sininen 20%"
                        }
                    ]
                }
            }
        }
    }
}
