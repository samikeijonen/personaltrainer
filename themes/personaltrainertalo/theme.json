{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "custom": {
            "color": {
                "primary": {
                    "100": "hsl(360, 100%, 97%)",
                    "200": "hsl(360, 82%, 89%)",
                    "300": "hsl(360, 77%, 78%)",
                    "400": "hsl(360, 71%, 66%)",
                    "500": "hsl(360, 64%, 55%)",
                    "600": "hsl(360, 67%, 44%)",
                    "700": "hsl(360, 72%, 38%)",
                    "800": "hsl(360, 79%, 32%)",
                    "900": "hsl(360, 85%, 25%)",
                    "1000": "hsl(360, 92%, 20%)"
                },
                "grey": {
                    "100": "hsl(40, 23%, 97%)",
                    "200": "hsl(43, 13%, 90%)",
                    "300": "hsl(40, 15%, 80%)",
                    "400": "hsl(39, 11%, 69%)",
                    "500": "hsl(41, 8%, 61%)",
                    "600": "hsl(41, 8%, 44%)",
                    "700": "hsl(41, 9%, 35%)",
                    "800": "hsl(37, 11%, 28%)",
                    "900": "hsl(40, 13%, 23%)",
                    "1000": "hsl(42, 15%, 13%)"
                },
                "neutral": {
                    "white": "#fff"
                }
            },
            "text": {
                "step-m1": "clamp(0.83rem, 0.78rem + 0.24vw, 1.00rem)",
                "step-0": "clamp(1.00rem, 0.93rem + 0.36vw, 1.25rem)",
                "step-1": "clamp(1.20rem, 1.09rem + 0.53vw, 1.56rem)",
                "step-2": "clamp(1.44rem, 1.29rem + 0.75vw, 1.95rem)",
                "step-3": "clamp(1.73rem, 1.52rem + 1.04vw, 2.44rem)",
                "step-4": "clamp(2.07rem, 1.79rem + 1.42vw, 3.05rem)",
                "step-5": "clamp(2.49rem, 2.10rem + 1.93vw, 3.82rem)",
                "step-6": "clamp(2.99rem, 2.47rem + 2.59vw, 4.77rem)"
            },
            "line-height": {
                "text": "1.5",
                "heading": "1.2"
            },
			"font": {
				"sans-serif": "Lato, sans-serif;"
			},
            "width": {
                "site-max": "125rem",
                "container": "87.5rem",
                "container-wide": "87.5rem",
                "container-medium": "46rem",
                "container-small": "27.5rem"
            },
            "transition": {
                "3": "0.3s",
                "4": "0.4s"
            }
        },
        "border": {
            "color": false,
            "radius": false,
            "style": false,
            "width": false
        },
        "color": {
            "custom": false,
            "customGradient": false,
            "defaultPalette": false,
            "defaultGradients": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": []
        },
        "layout": {
            "contentSize": "var(--wp--custom--width--container-medium)",
            "wideSize": "var(--wp--custom--width--container-wide)"
        },
        "spacing": {
            "margin": false,
            "padding": false,
            "units": []
        },
        "typography": {
            "customFontSize": false,
            "fontStyle": false,
            "fontWeight": false,
            "lineHeight": false,
            "textDecoration": false,
            "textTransform": false,
            "dropCap": false,
            "fontFamilies": [],
            "fontSizes": []
        },
		"blocks": {
            "meomblocks/group": {
                "color": {
                    "palette": [
                        {
                            "slug": "lighter",
                            "color": "var(--wp--custom--color--grey--100)",
                            "name": "Lighter"
                        },
                        {
                            "slug": "light",
                            "color": "var(--wp--custom--color--grey--200)",
                            "name": "Light"
                        }
                    ]
                }
            }
        }
    }
}
